{
  "name": "new-project",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "watch": "tsc -p tsconfig.json --watch",
    "new_project": "schematics .:new-project --dryRun=false",
    "debug_new_project": "schematics .:new-project",
    "create_file": "schematics .:create-file --dryRun=false",
    "debug_create_file": "schematics .:create-file",
    "create_component": "schematics .:create-component --dryRun=false",
    "debug_create_component": "schematics .:create-component"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^13.2.2",
    "@angular-devkit/schematics": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@schematics/angular": "^13.2.2",
    "typescript": "~4.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine": "^4.0.0"
  }
}
